<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity AssocMember.
    -->
    <changeSet id="20230520180521-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="pledge_id"
                                 baseTableName="assoc_member"
                                 constraintName="fk_assoc_member__pledge_id"
                                 referencedColumnNames="id"
                                 referencedTableName="pledge"/>

        <addForeignKeyConstraint baseColumnNames="assoc_member_id"
                                 baseTableName="rel_assoc_member__association"
                                 constraintName="fk_rel_assoc_member__association__assoc_member_id"
                                 referencedColumnNames="id"
                                 referencedTableName="assoc_member"/>

        <addForeignKeyConstraint baseColumnNames="association_id"
                                 baseTableName="rel_assoc_member__association"
                                 constraintName="fk_rel_assoc_member__association__association_id"
                                 referencedColumnNames="id"
                                 referencedTableName="association"/>
    </changeSet>
</databaseChangeLog>
